        "uglifyjs-webpack-plugin": "^0.4.3",
        "webpack": "^2.5.1",
        "webpack-auto-inject-version": "^0.5.14"
    },
    "scripts": {
        "build": "npm-run uglifycss pwa/src/style.css > pwa/style.min.css & npm-run webpack --env.minor & npm-run html-minifier pwa/src/index.html -o pwa/index.html --collapse-whitespace --remove-comments"
